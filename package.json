{
  "name": "preact-app",
  "version": "0.0.0",
  "description": "",
  "main": "./server",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "webpack",
    "clean": "rm -rf ./build/*",
    "predev": "npm run clean",
    "dev": "node --inspect=0.0.0.0:9229 ./node_modules/.bin/webpack-dev-server",
    "lint:css": "prettier **/*.{css,sass,scss}",
    "lint:ts": "eslint --ext .cjs,.cjsx,.js,.jsx,.mjs,.mjsx,.ts,.tsx .",
    "lint": "npm run lint:ts -- --fix && npm run lint:css -- --write",
    "prod": "NODE_ENV=production npm run build && NODE_ENV=production npm run start",
    "start": "node .",
    "test": "npm audit && tsc --noEmit && npm run lint:ts && npm run lint:css -- --check && jest --coverage",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "watch": "npm run build -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "source-map-support": "^0.5.16",
    "styled-components": "^4.4.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@testing-library/react": "^9.4.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.1",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.0.2",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preact": "^10.1.1",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "react-svg-loader": "^3.0.3",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "supertest": "^4.0.2",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^2.3.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "whatwg-fetch": "^3.0.0",
    "yaml-loader": "^0.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/server/*.{js,ts}",
      "src/server/app.ts",
      "!src/server/router/auth/**/*",
      "!src/**/*.d.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^~/(.*)": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ]
  },
  "lint-staged": {
    "**/*.{css,sass,scss}": [
      "prettier",
      "git add"
    ],
    "**/*.{cjs,cjsx,js,jsx,mjs,mjsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
}
