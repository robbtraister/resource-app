{
  "name": "express-server",
  "version": "0.0.0",
  "description": "",
  "main": "./server",
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rm -rf ./build/*",
    "dev": "npm run clean && node --inspect=0.0.0.0:9229 ./node_modules/.bin/webpack-dev-server",
    "lint:ts": "eslint --ext .cjs,.js,.mjs,.ts .",
    "lint": "npm run lint:ts -- --fix",
    "prod": "NODE_ENV=production npm run build && NODE_ENV=production npm run start",
    "start": "node .",
    "test": "npm audit && tsc --noEmit && npm run lint:ts && jest --coverage",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags",
    "watch": "npm run build -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "supertest": "^4.0.2",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,ts}",
      "!src/server/*.{js,ts}",
      "src/server/app.ts",
      "!src/server/router/auth/**/*",
      "!src/**/*.d.ts"
    ],
    "moduleNameMapper": {
      "^~/(.*)": "<rootDir>/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ]
  },
  "lint-staged": {
    "**/*.{cjs,js,mjs,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
}
